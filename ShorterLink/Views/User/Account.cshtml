@{
    ViewData["Title"] = "Account";
}

<div class="container w-75 p-5 bg-white rounded enter-link-controls">
  <h1>Account Data</h1>
  <div id="username"><span><strong>Username:</strong>@Context.Session.GetString("username")</span></div>
  <div id="email"><span><strong>Email:</strong>@Context.Session.GetString("email")</span></div>
  <div id="singout"><a class="btn btn-danger" href="/user/signout">Sign Out</a></div>
</div> 